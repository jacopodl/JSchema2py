{
  "$schema": "http://json-schema.org/draft-06/schema#",
  "title": "BasicProperties",
  "type": "object",
  "properties": {
    "string": {
      "type": "string",
      "default": "HelloWorld"
    },
    "number": {
      "type": "number",
      "default": 24.00
    },
    "boolean": {
      "type": "boolean",
      "default": true
    },
    "null": {
      "type": "null"
    },
    "integer": {
      "type": "integer",
      "default": 98
    }
  }
}